{
  "name": "heros-et-dragons-drs",
  "version": "1.0.1",
  "description": "Document de Référence Système pour Héros & Dragons",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erlik/heros-et-dragons-drs.git"
  },
  "author": "Maxime Moraine",
  "license": "MIT",
  "scripts": {
    "dev": "vuepress dev docs",
    "check-md": "vuepress check-md docs",
    "electron": "wait-on http://localhost:8080 && electron .",
    "electron-dev": "concurrently \"yarn run dev\" \"yarn run electron\"",
    "preelectron-pack": "yarn build",
    "postinstall": "electron-builder install-app-deps",
    "build": "electron-builder --publish never",
    "release": "electron-builder"
  },
  "dependencies": {
    "@mdi/font": "^5.9.55",
    "@vuepress/plugin-blog": "^1.9.4",
    "@vuepress/plugin-google-analytics": "^1.8.1",
    "@vuepress/plugin-pwa": "^1.8.1",
    "axios": "^0.21.1",
    "electron-serve": "^1.1.0",
    "esm": "^3.2.25",
    "file-saver": "^2.0.5",
    "flexsearch": "nextapps-de/flexsearch",
    "js-cookie": "^2.2.1",
    "markdown-it-div": "^1.1.0",
    "markdown-it-multimd-table": "^4.0.3",
    "material-design-icons-iconfont": "^5.0.1",
    "node-sass": "^4.14.1",
    "sass-loader": "^8.0.2",
    "slugify": "^1.4.6",
    "vue": "^2.6.12",
    "vue-masonry-css": "^1.0.3",
    "vuepress-plugin-clean-urls": "^1.1.2",
    "vuepress-plugin-dehydrate": "^1.1.5",
    "vuepress-plugin-seo": "^0.1.4",
    "vuepress-plugin-sitemap": "^2.3.1",
    "vuetify": "^2.4.4",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "electron": "^11.2.3",
    "electron-builder": "^22.9.1",
    "vuepress": "^1.8.1",
    "vuepress-plugin-check-md": "^0.0.2",
    "wait-on": "^5.2.1"
  },
  "main": "main.js",
  "build": {
    "appId": "org.erlik.heros-et-dragons-drs",
    "productName": "Héros & Dragons DRS",
    "icon": "docs/.vuepress/public/favicon.png",
    "compression": "maximum",
    "files": [
      "build/**/*",
      "main.js"
    ],
    "publish": {
      "provider": "github"
    },
    "mac": {
      "category": "public.build.automation",
      "artifactName": "heros-et-dragons-drs-electron-installer-${version}.${ext}"
    },
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 440,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ],
      "artifactName": "heros-et-dragons-drs-electron-installer-${version}.${ext}"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "portable"
        }
      ]
    },
    "nsis": {
      "artifactName": "heros-et-dragons-drs-electron-installer-${version}.${ext}"
    },
    "portable": {
      "artifactName": "heros-et-dragons-drs-electron-${version}.${ext}"
    },
    "linux": {
      "target": "AppImage",
      "artifactName": "heros-et-dragons-drs-electron-${version}.${ext}"
    }
  }
}
